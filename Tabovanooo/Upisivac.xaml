<Window x:Class="Tabovanooo.Upisivac"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tabovanooo"
        mc:Ignorable="d"
        Title="Upisivac" Height="450" Width="800" FontSize="24">
	<UniformGrid Columns="3">
	    <ListBox Name="listbPolaznici"/>
		<StackPanel>
			<Label Content="{Binding Path=Naziv}"/>
			<Button Name="Tamo" Click="Upis"/>
			<Button Name="Ovamo"/>
		</StackPanel>
		<ListBox Name="listbKurs"/>
	</UniformGrid>
</Window>
